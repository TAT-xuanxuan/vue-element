{
  "name": "vue-element",
  "version": "1.0.0",
  "description": "use vue and element-ui to build a web project without vue-cli!",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "test": "cross-env NODE_ENV=test webpack-dev-server --config webpack.config.js",
    "pro": "cross-env NODE_ENV=production webpack-dev-server --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TAT-xuanxuan/vue-element.git"
  },
  "keywords": [
    "vue",
    "element",
    "less",
    "webpack"
  ],
  "author": "wangxy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TAT-xuanxuan/vue-element/issues"
  },
  "homepage": "https://github.com/TAT-xuanxuan/vue-element#readme",
  "dependencies": {
    "cross-env": "^5.2.0",
    "element-ui": "^2.9.2",
    "html-webpack-plugin": "^3.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "webpack": "^4.32.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
