    "sass-resources-loader": "^2.0.1",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "url-loader": "^1.1.2",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.2",
    "webpack-html-plugin": "^0.1.1"
  },
  "browserslist": [
    "defaults",
